<?xml version="1.0" encoding="utf-8" ?>
<Application
    xmlns="http://xamarin.com/schemas/2014/forms"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
    x:Class="AiFormsDialogsXF.App">
    <Application.Resources>
        <ResourceDictionary>
            <OnPlatform x:TypeArguments="x:String" x:Key="ComicSans">
                <On Platform="Android" Value="Comic.ttf#Comic" />
                <On Platform="iOS" Value="Comic-Sans" />
            </OnPlatform>
        </ResourceDictionary>
    </Application.Resources>
</Application>